/run/initialize

/control/verbose 2
/run/verbose 2
/tracking/verbose 0
#点源输入
/gps/particle neutron

/gps/pos/type Plane
/gps/pos/shape Rectangle
/gps/pos/centre 0 2 0 cm
/gps/pos/halfx 10. cm
/gps/pos/halfz 10. cm

#粒子方向
/gps/ang/type planar
/gps/direction 0 0 -1

#能谱输入

/gps/ene/type Pow
/gps/ene/min 0.01 eV
/gps/ene/max 10. MeV
/gps/ene/alpha -1.

#/gps/hist/inter

/run/beamOn 100000
